#include<stdio.h>
#include<stdint.h>
char arr[24] = " Toi ten la van viet ke";
int temp = 0;
int main()
{
    for ( int i = 24; i >=0; i--)
    {
     temp++;
     if (arr[i] ==' ')
    {
        for (int j = i+1 ; j<i+temp; j++)
        {
            printf("%c", arr[j]);
        }
        printf(" ");
        temp = 0;     
    }
    }
